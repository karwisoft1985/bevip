<?xml version='1.0' encoding='utf-8'?>
<widget id="com.karwisoft.bevip2016" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>bevip</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="res/icon.png" />
        <splash density="land-hdpi" src="res/splash.png" />
        <splash density="land-ldpi" src="res/splash.png" />
        <splash density="land-mdpi" src="res/splash.png" />
        <splash density="land-xhdpi" src="res/splash.png" />
        <splash density="port-hdpi" src="res/splash.png" />
        <splash density="port-ldpi" src="res/splash.png" />
        <splash density="port-mdpi" src="res/splash.png" />
        <splash density="port-xhdpi" src="res/splash.png" />
    </platform>
    <platform name="ios">
        <splash height="480" src="res/screen/ios/Default~iphone.png" width="320" />
        <splash height="960" src="res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="1024" src="res/screen/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="2048" src="res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="768" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1136" src="res/screen/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/screen/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/screen/ios/Default-736h.png" width="1242" />
        <splash height="1242" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
    </platform>
    <platform name="wp8">
        <splash height="1280" src="res/screen/wp8/SplashScreenImage.jpg" width="768" />
    </platform>
    <platform name="windows8">
        <splash height="300" src="res/screen/windows8/splashscreen.png" width="620" />
    </platform>
    <platform name="blackberry10">
        <rim:splash src="res/screen/windows8/splashscreen.png" />
    </platform>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="4000" />
    <plugin name="org.apache.cordova.plugin.cache" spec="https://github.com/tiltshiftfocus/cordova-plugin-cache.git" />
    <plugin name="cordova-plugin-keyboard" spec="~1.2.0" />
    <plugin name="cordova-plugin-audio-recorder-api" spec="~0.0.1" />
    <plugin name="cordova-plugin-file-opener2" spec="~2.0.19" />
    <plugin name="com-badrit-macaddress" spec="https://github.com/mohamed-salah/MacAddress.git" />
    <plugin name="cordova-plugin-nativeaudio" spec="~3.0.9" />
    <plugin name="cordova-plugin-calendar" spec="^5.0.0" />
    <plugin name="cordova-android-support-gradle-release" spec="^1.4.4">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <engine name="android" spec="^6.3.0" />
</widget>
